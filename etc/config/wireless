
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'ffe0a000.pcie/pcia000:02/a000:02:00.0/a000:03:00.0'
	option channel 'auto'
	option band '5g'
	option htmode 'VHT80'
	option cell_density '0'
	option country 'DE'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Nerdberg'
	option encryption 'psk2+ccmp'
	option dtim_period '3'
	option key '%WPA2_PSK%'
	option ieee80211r '1'
	option mobility_domain '6e62'
	option reassociation_deadline '2000'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option ieee80211k '1'
	option bss_transition '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'ffe09000.pcie/pci9000:00/9000:00:00.0/9000:01:00.0'
	option channel 'auto'
	option band '2g'
	option htmode 'HT40'
	option country 'DE'
	option cell_density '0'
	option noscan '1'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'Nerdberg'
	option encryption 'psk2+ccmp'
	option dtim_period '3'
	option key '%WPA2_PSK%'
	option ieee80211r '1'
	option mobility_domain '6e62'
	option reassociation_deadline '20000'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option ieee80211k '1'
	option bss_transition '1'

config wifi-iface 'wifinet2'
	option device 'radio0'
	option mode 'ap'
	option ssid 'freifunk-nuernberg.de#stadt'
	option encryption 'none'
	option dtim_period '3'
	option ieee80211k '1'
	option bss_transition '1'
	option network 'freifunk'

config wifi-iface 'wifinet3'
	option device 'radio1'
	option mode 'ap'
	option ssid 'freifunk-nuernberg.de#stadt'
	option encryption 'none'
	option dtim_period '3'
	option ieee80211k '1'
	option bss_transition '1'
	option network 'freifunk'

